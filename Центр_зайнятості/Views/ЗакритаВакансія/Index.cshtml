@model IEnumerable<Центр_зайнятості.Models.ЗакритаВакансія>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.НазваВакансії)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ДатаЗакриття)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ВлаштованийNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.КодВакансіїNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.НазваВакансії)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ДатаЗакриття)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ВлаштованийNavigation.КодПрацівника)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.КодВакансіїNavigation.КодВакансії)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.НомерЗапису">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.НомерЗапису">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.НомерЗапису">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
